{
    "name": "Copy Odds",
    "description": "Copy Odds from Bookmakers",
    "version": "0.1",
    "manifest_version": 3,
    "permissions": [],
    "content_scripts": [
      {
        "js": ["content_script.js"],
        "matches": [
          "https://www.pinnacle.com/en/*",
          "https://www.pinnacle.ca/en/*"
